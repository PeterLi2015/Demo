<div id="main">
    <label class="form-control form-control-inline">选择级别</label>
    <select class="form-control form-control-inline" v-model="level" v-on:change="selectlevel(level)">
        <option v-for="level in levels" v-bind:value="level">
            {{level}}
        </option>
    </select>
</div>
<br />
