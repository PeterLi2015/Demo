@using PagedList;
@using PagedList;
@using XDropsWater.Model;
@using PagedList.Mvc;
@using System.Web.Mvc;
@using System.Web.Handlers;
@model IPagedList<SubMemberModel>


<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>手机号码</th>
            <th>姓名</th>
            <th>代理级别</th>
            <th>层级关系</th>
            <th>注册时间</th>
            <th>住址</th>
        </tr>
    </thead>
    <tbody id="tableBody">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Mobile</td>
                <td>@item.MemberName</td>
                <td>@item.RoleName</td>
                <td>@item.LevelID</td>
                <td>@item.CreateOn</td>
                <td>@item.Address</td>
                
            </tr>
        }
    </tbody>
</table>
<div id="page" style="margin-left: 10px">
    第 @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) 页
    共 @Model.PageCount 页

    @Html.PagedListPager(Model, page => Url.Action("GetHighSubMembers", new { page }), new PagedListRenderOptions { LinkToFirstPageFormat = "第一页", LinkToPreviousPageFormat = "上一页", LinkToNextPageFormat = "下一页", LinkToLastPageFormat = "最后一页", MaximumPageNumbersToDisplay = 7 })
</div>

